<?xml version='1.0' encoding='utf-8'?>
<widget id="com.feelhungry.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>FEELHUNGRY</name>
    <description>
        FEELHUNGRY
    </description>
    <author email="ahmednasa4@gmail.com" href="">
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreen" value="splash.png" />
    <preference name="SplashMaintainAspectRatio" value="true" />
</widget>
